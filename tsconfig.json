{
  "compilerOptions": {
    //required
    "baseUrl": "src",
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    //required    


    // "baseUrl": "src",
    // "target": "ESNext",
    // "module": "ESNext",
    // "lib": ["dom"],
    // "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    // "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    // "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    // "noEmit": true,
  },
  "exclude": ["node_modules", "public", ".cache"],
}