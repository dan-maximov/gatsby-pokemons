{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": true,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  }
}
